{
  "version": 2,
  "name": "Totspots",
  "alias": "Totspots",
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "^/favicon.ico$",
      "dest": "/favicon.ico"
    },
    {
      "src": "^/manifest.json$",
      "dest": "/manifest.json"
    },
    {
      "src": "^/site.webmanifest.json$",
      "dest": "/site.webmanifest.json"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_ENDPOINT": "https://radiant-island-73758.herokuapp.com"
  }
}